<ul class="products__list">
    {{#each this}}
    <li class="products__item">
        <article class="product">
            <h2 class="product__title">{{name}}</h2>
            <p class="product__price">{{price}}</p>
            <p class="product__description">{{description}}</p>
        </article>
    </li>
    {{/each}}
</ul>